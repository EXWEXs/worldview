
{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "script",
      "ecmaFeatures": {
          "jsx": false
      }
  },
  "rules": {
      "strict": 0,
      "semi": [2, "always"],
      "indent": [2, 2, {"SwitchCase": 1}],
  }
}
